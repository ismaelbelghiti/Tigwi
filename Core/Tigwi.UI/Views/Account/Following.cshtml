@model Tigwi.UI.Models.IAccountModel

@{
    ViewBag.Title = "Tigwi | "+Model.Name+" friends";
}

<h2>@Model.Name 's Followed Lists</h2>

@foreach(var followedList in Model.PublicFollowedLists)
{
    <h3>@followedList.Name </h3>
    <p> 
    Description : @followedList.Description
    </p>
    @Html.Partial("_FollowList", followedList)
    
}